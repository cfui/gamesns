<div class="container">
  <div class="row">
    <div class="col-xs-4"></div>
    <div class="col-xs-2">
	  <h3>DMした人一覧</h3>
	  <table class="table table-striped">
		<tbody>
		  <% @anotherEntries.each do |entry| %>
			<tr>
			  <th>
				<%= link_to dm_room_path(entry.dm_room.id) do %>
				<%= attachment_image_tag entry.enduser, 
				:profile_image, :fill, 100, 100, fallback: "no_image.jpg" %><br>
				<%= entry.enduser.nickname %>さんとDMする<br>
			  </th>
			</tr>
		    <% end %>
		  <% end %>
		</tbody>
	</table>
	<%= paginate @anotherEntries, class: "paginate" %>
    </div>
    <div class="col-xs-4"></div>
  </div>
</div>

